{
    "name": "gerardojbaez/laraplans",
    "description": "SaaS style recurring plans for Laravel.",
    "keywords": ["plans", "laravel", "subscriptions", "memberships"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.1.3|^8.0",
        "nesbot/carbon": "^2.53.1",
        "illuminate/support": "^8.6|^9.0",
        "illuminate/database": "^8.6|^9.0",
        "laravel/helpers": "^1.5"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^9.5.10",
        "fakerphp/faker": "^1.9.1",
        "orchestra/testbench": "^5.20|^6.23",
        "laravel/legacy-factories": "1.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Gerardojbaez\\Laraplans\\": "src/Laraplans/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "Gerardojbaez\\Laraplans\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
